@article{ WOS:001166549700001,
Author = {Avellaneda-Tamayo, Juan F. and Chavez-Hernandez, Ana L. and
   Prado-Romero, Diana L. and Medina-Franco, Jose L.},
Title = {Chemical Multiverse and Diversity of Food Chemicals},
