@article{ WOS:001556094500001,
Author = {Karamitas, Chariton and Kehagias, Athanasios},
Title = {REcover: towards recovering object files from stripped binary
   executables},
Journal = {JOURNAL OF COMPUTER VIROLOGY AND HACKING TECHNIQUES},
Year = {2025},
Volume = {21},
Number = {1},
Month = {AUG 22},
Abstract = {Reverse engineering complex proprietary software is a tedious and time
   consuming task. A fair amount of the overall effort is usually devoted
   to locating those software components which are responsible for the
   functionality of interest (e.g., a proprietary encryption algorithm). To
   aid this process, several tools, available in the public domain, can be
   used, implementing sophisticated algorithms for control-flow recovery,
