{
  "version": "1.0",
  "steps": [
    {
      "id": "dedup_doi",
      "type": "dedup-doi",
      "name": "DOI Deduplication",
      "enabled": true,
      "input_from": "sources",
      "config": {
        "case_sensitive": false,
        "keep_no_doi": true
      }
    },
    {
      "id": "dedup_title",
      "type": "dedup-title",
      "name": "Title Deduplication",
      "enabled": true,
      "input_from": "dedup_doi",
      "config": {
        "similarity_threshold": 0.92
      }
    },
    {
      "id": "dedup_author",
      "type": "dedup-author",
      "name": "Author Deduplication",
      "enabled": true,
      "input_from": "dedup_title",
      "config": {
        "similarity_threshold": 0.8
      }
    }
  ],
  "final_output": null
}