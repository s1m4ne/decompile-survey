@article{ WOS:000737527800001,
Author = {Guan, Jun and Jiang, Xu and Mao, Baolei},
Title = {A Method for Class-Imbalance Learning in Android Malware Detection},
Journal = {ELECTRONICS},
Year = {2021},
Volume = {10},
Number = {24},
Month = {DEC},
Abstract = {More and more Android application developers are adopting many different
   methods against reverse engineering, such as adding a shell, resulting
   in certain features that cannot be obtained through decompilation, which
   causes a serious sample imbalance in Android malware detection based on
   machine learning. Hence, the researchers have focused on how to solve
   class-imbalance to improve the performance of Android malware detection.
   However, the disadvantages of the existing class-imbalance learning are
   mainly the loss of valuable samples and the computational cost. In this
   paper, we propose a method of Class-Imbalance Learning (CIL), which
   first selects representative features, uses the clustering K-Means
   algorithm and under-sampling to retain the important samples of the
   majority class while reducing the number of samples of the majority
