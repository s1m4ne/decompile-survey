@article{ WOS:001102195500001,
Author = {Rodriguez-Bazan, Horacio and Sidorov, Grigori and Escamilla-Ambrosio,
   Ponciano Jorge},
Title = {Android Ransomware Analysis Using Convolutional Neural Network and Fuzzy
   Hashing Features},
Journal = {IEEE ACCESS},
Year = {2023},
