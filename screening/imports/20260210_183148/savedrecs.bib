@article{ WOS:001429826200001,
Author = {Thabit, Hamdan and Ahmad, Rami and Abdullah, Ahmad and Abualkishik,
   Abedallah Zaid and Alwan, Ali A.},
Title = {Detecting Malicious .NET Executables Using Extracted Methods Names},
